{
  "expo": {
    "name": "Notes",
    "slug": "Notes",
    "owner": "tancetiner",
    "description": "This is an app for taking notes on your phone.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/notesCover.png",
    "splash": {
      "image": "./assets/notesLoad.png",
      "resizeMode": "contain",
      "backgroundColor": "#D1E1BC"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
